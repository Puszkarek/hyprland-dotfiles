{
  "editor.bracketPairColorization.enabled": true,
  "javascript.suggest.autoImports": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "javascript.suggest.includeCompletionsForImportStatements": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "extensions.ignoreRecommendations": true,
  "typescript.tsserver.log": "off",
  "editor.formatOnSave": true,
  "editor.autoClosingBrackets": "never",
  "editor.linkedEditing": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.suggest.insertMode": "replace",
  "editor.fontFamily": "Share Tech Mono",
  "terminal.integrated.fontFamily": "Share Tech Mono",
  "editor.codeLensFontFamily": "Share Tech Mono",
  "chat.editor.fontFamily": "Share Tech Mono",
  "editor.fontSize": 18,
  "editor.lineHeight": 1.6,
  "editor.renderLineHighlight": "gutter",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": true
  },
  "zenMode.restore": true,
  "window.titleBarStyle": "custom",
  "window.customTitleBarVisibility": "never",
  "window.titleSeparator": " â˜£ ",
  "git.mergeEditor": false,
  "json.maxItemsComputed": 1000,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "html.format.wrapAttributes": "force-expand-multiline",
  "eslint.format.enable": true,
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Windows PowerShell": {
      "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    }
  },
  "git.autofetch": true,
  "editor.inlineSuggest.enabled": true,
  "terminal.integrated.scrollback": 5000,
  "gitlens.views.commits.files.layout": "tree",
  "editor.defaultFormatter": "biomejs.biome",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "editor.hover.enabled": true,
  "git.suggestSmartCommit": false,
  "debug.terminal.clearBeforeReusing": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "diffEditor.maxComputationTime": 0,
  "workbench.startupEditor": "none",
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    "*.tsx": "typescriptreact",
    ".env.*": "dotenv",
    ".prettierrc": "json"
  },
  "editor.parameterHints.enabled": false,
  "cSpell.language": "en,pt-BR",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.suggestSelection": "first",
  "explorer.confirmDelete": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "terminal.integrated.showExitAlert": false,
  "typescript.suggest.autoImports": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "workbench.editor.labelFormat": "short",
  "editor.fontLigatures": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "git.enableSmartCommit": true,
  "editor.accessibilitySupport": "off",
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.fontSize": 14,
  "editor.semanticHighlighting.enabled": false,
  "breadcrumbs.enabled": true,
  "breadcrumbs.icons": false,
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showClasses": true,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showConstructors": false,
  "breadcrumbs.showEnums": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showNamespaces": false,
  "breadcrumbs.symbolSortOrder": "type",
  "breadcrumbs.symbolPath": "on",
  "breadcrumbs.filePath": "on",
  "gitlens.codeLens.authors.enabled": false,
  "editor.tabSize": 2,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "files.exclude": {
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.git": true
  },
  "gitlens.codeLens.enabled": false,
  "terminal.explorerKind": "external",
  "workbench.editor.tabSizing": "shrink",
  "typescript.tsserver.maxTsServerMemory": 1024,
  "cSpell.blockCheckingWhenLineLengthGreaterThan": 2000,
  "cSpell.spellCheckDelayMs": 300,
  "typescript.tsserver.enableTracing": true,
  "editor.maxTokenizationLineLength": 800,
  "eslint.probe": ["javascript", "typescript", "html", "markdown"],
  "eslint.timeBudget.onValidation": {
    "warn": 1000,
    "error": 1000
  },
  "eslint.useESLintClass": true,
  "eslint.trace.server": "off",
  "workbench.iconTheme": null,
  "editor.stickyTabStops": true,
  "terminal.integrated.gpuAcceleration": "on",
  "symbols.hidesExplorerArrows": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": ".eslint*, biome.json, .prettier*, tsconfig*, .npmrc*, pnpm-lock*, package-lock*",
    "jest.config.*": "jest.preset*",
    "Dockerfile.api.prod": ".dockerignore, docker-compose*, docker-entrypoint, Dockerfile.api*",
    "fly.prod.toml": "fly.*",
    ".env.local": ".env*",
    ".env": ".env*",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "editor.minimap.autohide": false,
  "editor.overviewRulerBorder": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.renderCharacters": false,
  "workbench.tree.stickyScrollMaxItemCount": 4,
  "terminal.integrated.stickyScroll.maxLineCount": 4,
  "editor.stickyScroll.maxLineCount": 3,
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "editor.minimap.size": "fill",
  "editor.inlineSuggest.suppressSuggestions": true,
  "remote.autoForwardPortsSource": "hybrid",
  "diffEditor.ignoreTrimWhitespace": false,
  "cSpell.userWords": ["Schedulable"],
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "cSpell.spellCheckOnlyWorkspaceFiles": true,
  "editor.stickyScroll.enabled": false,
  "workbench.activityBar.location": "top",
  "notebook.lineNumbers": "on",
  "editor.colorDecoratorsLimit": 5000,
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "Midnight Retro",
  "workbench.statusBar.visible": false,
  "markdown.preview.fontFamily": "Share Tech Mono"
}
